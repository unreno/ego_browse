<p id="notice"><%= notice %></p>

<h1>Interviews</h1>

<table>
  <thead>
    <tr>
      <th>Active</th>
      <th>Studyid</th>
      <th>Completed</th>
      <th>Start date</th>
      <th>Complete date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interviews.each do |interview| %>
      <tr>
        <td><%= interview.active %></td>
        <td><%= interview.studyId %></td>
        <td><%= interview.completed %></td>
        <td><%= interview.start_date %></td>
        <td><%= interview.complete_date %></td>
        <td><%= link_to 'Show', interview %></td>
      </tr>
    <% end %>
  </tbody>
</table>
