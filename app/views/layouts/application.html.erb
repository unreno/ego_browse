<!DOCTYPE html>
<html>
	<head>
		<title>EgoBrowse</title>
		<%= csrf_meta_tags %>

		<%= stylesheet_link_tag		'application', media: 'all', 'data-turbolinks-track': 'reload' %>
		<%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
	</head>

	<body>

<% if current_rails_user %>
  <p><%= link_to "Logout", rails_user_session_path, method: :delete, data: { confirm: "Are you sure you want to logout?" } %></p>
<% end %>
		<% flash.keys.each do |key| %>
			<p class='flash <%=key-%>'><%=flash[key]-%></p>
		<% end	#flash.keys.each do |key| %>
		<%= yield %>
	</body>
</html>
