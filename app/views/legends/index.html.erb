<p id="notice"><%= notice %></p>

<h1>Legends</h1>

<table>
  <thead>
    <tr>
      <th>Studyid</th>
      <th>Questionid</th>
      <th>Shape</th>
      <th>Label</th>
      <th>Color</th>
      <th>Size</th>
      <th>Ordering</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @legends.each do |legend| %>
      <tr>
        <td><%= legend.studyId %></td>
        <td><%= legend.questionId %></td>
        <td><%= legend.shape %></td>
        <td><%= legend.label %></td>
        <td><%= legend.color %></td>
        <td><%= legend.size %></td>
        <td><%= legend.ordering %></td>
        <td><%= link_to 'Show', legend %></td>
      </tr>
    <% end %>
  </tbody>
</table>
