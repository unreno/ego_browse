<p id="notice"><%= notice %></p>

<h1>Answers</h1>

<table>
  <thead>
    <tr>
      <th>Active</th>
      <th>Questionid</th>
      <th>Interviewid</th>
      <th>Alterid1</th>
      <th>Alterid2</th>
      <th>Value</th>
      <th>Otherspecifytext</th>
      <th>Skipreason</th>
      <th>Studyid</th>
      <th>Questiontype</th>
      <th>Answertype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answers.each do |answer| %>
      <tr>
        <td><%= answer.active %></td>
        <td><%= link_to answer.questionId, question_path( answer.questionId ) %></td>
        <td><%= link_to answer.interviewId, interview_path( answer.interviewId ) %></td>
        <td><%= link_to answer.alterId1, alter_path( answer.alterId1 ) %></td>
        <td><%= link_to answer.alterId2, alter_path( answer.alterId2 ) %></td>
        <td><%= answer.value %></td>
        <td><%= answer.otherSpecifyText %></td>
        <td><%= answer.skipReason %></td>
        <td><%= link_to answer.studyId, study_path( answer.studyId ) %></td>
        <td><%= answer.questionType %></td>
        <td><%= answer.answerType %></td>
        <td><%= link_to 'Show', answer %></td>
      </tr>
    <% end %>
  </tbody>
</table>
