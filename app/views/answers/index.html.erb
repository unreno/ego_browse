<p id="notice"><%= notice %></p>

<h1>Answers</h1>

<table>
  <thead>
    <tr>
      <th>Active</th>
      <th>Questionid</th>
      <th>Interviewid</th>
      <th>Alterid1</th>
      <th>Alterid2</th>
      <th>Value</th>
      <th>Otherspecifytext</th>
      <th>Skipreason</th>
      <th>Studyid</th>
      <th>Questiontype</th>
      <th>Answertype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answers.each do |answer| %>
      <tr>
        <td><%= answer.active %></td>
        <td><%= answer.questionId %></td>
        <td><%= answer.interviewId %></td>
        <td><%= answer.alterId1 %></td>
        <td><%= answer.alterId2 %></td>
        <td><%= answer.value %></td>
        <td><%= answer.otherSpecifyText %></td>
        <td><%= answer.skipReason %></td>
        <td><%= answer.studyId %></td>
        <td><%= answer.questionType %></td>
        <td><%= answer.answerType %></td>
        <td><%= link_to 'Show', answer %></td>
      </tr>
    <% end %>
  </tbody>
</table>
