<p id="notice"><%= notice %></p>

<h1>Alter Prompts</h1>

<table>
	<thead>
		<tr>
			<th>Study ID</th>
			<th>Afteraltersentered</th>
			<th>Display</th>
			<th>&nbsp;</th>
		</tr>
	</thead>

	<tbody>
		<% @alter_prompts.each do |alter_prompt| %>
			<tr>
				<td><%= link_to alter_prompt.studyId, alter_prompt.study %></td>
				<td><%= alter_prompt.afterAltersEntered %></td>
				<td><%= alter_prompt.display %></td>
				<td><%= link_to 'Show', alter_prompt %></td>
			</tr>
		<% end %>
	</tbody>
</table>
