<p id="notice"><%= notice %></p>

<h1>Notes</h1>

<table>
  <thead>
    <tr>
      <th>Interviewid</th>
      <th>Expressionid</th>
      <th>Alterid</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <td><%= note.interviewId %></td>
        <td><%= note.expressionId %></td>
        <td><%= note.alterId %></td>
        <td><%= note.notes %></td>
        <td><%= link_to 'Show', note %></td>
      </tr>
    <% end %>
  </tbody>
</table>
