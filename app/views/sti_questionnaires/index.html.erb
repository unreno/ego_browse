<p id="notice"><%= notice %></p>

<h1>STI Questionnaires
<% if current_user.present? and current_user.is_admin? %>
[ <%= link_to 'Download CSV', sti_questionnaires_path(format:'csv') %> ]
<% end %>
<% if current_user.present? and current_user.can_create? %>
[ <%= link_to 'New Sti Questionnaire', new_sti_questionnaire_path %> ]
<% end %>
</h1>
<br>

