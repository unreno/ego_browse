			<th>Notest reason H</th>
			<th>Notest reason I</th>
			<th>Notest reason other</th>
			<th>Stitest future</th>
			<th>Stipercrisk</th>
			<th>Stistigma</th>
		</tr>
	</thead>

	<tbody>
		<% @sti_questionnaires.each do |sti_questionnaire| %>
			<tr>
				<td><%= link_to 'Show', sti_questionnaire %>
				<% if current_user.present? and current_user.can_update? %>
					<%= link_to 'Edit', edit_sti_questionnaire_path(sti_questionnaire) %>
				<% end %>
